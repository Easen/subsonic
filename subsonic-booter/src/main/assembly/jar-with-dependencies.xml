<assembly>
    <id>jar-with-dependencies</id>
    <formats>
        <format>jar</format>
    </formats>
    <includeBaseDirectory>false</includeBaseDirectory>
    <dependencySets>

        <dependencySet>
            <outputDirectory>/</outputDirectory>
            <unpack>true</unpack>
            <scope>runtime</scope>
            <excludes>
                <exclude>org.mortbay.jetty:jsp-2.0</exclude>
            </excludes>
        </dependencySet>

    </dependencySets>
</assembly>
